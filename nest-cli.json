{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": {
      "type": "swc",
      "options": {
        "swcrcPath": ".swcrc",
        "swcCliPath": "./node_modules/.bin/swc",
        "swcPath": "./node_modules/@swc/core"
      }
    },
    "typeCheck": true,
    "deleteOutDir": true
  }
}